{
  "name": "Artists",
  "version": "0.0.0",
  "description": "Artists",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config webpack.config.babel.js",
    "start": "webpack-dev-server --progress --hot --color --config webpack.config.babel.js",
    "eslint": "eslint . ",
    "stylelint": "stylelint '**/*.scss' --syntax scss",
    "stylefmt": "stylefmt -R .",
    "lint": "npm run stylelint && npm run eslint",
    "jest": "jest",
    "test": "npm run lint && jest"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(scss)$": "<rootDir>/components/mock/styles.js",
      "^.+\\.(png)$": "<rootDir>/components/mock/empty"
    }
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "html-minify-loader": "^1.1.0",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^15.1.1",
    "node-sass": "^3.10.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.4",
    "react-test-renderer": "^15.3.2",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.3.1",
    "stylelint-config-primer": "^1.2.0",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.16.1",
    "file-loader": "^0.9.0",
    "jest": "^15.1.1",
    "jest-cli": "^16.0.2",
    "react-test-renderer": "^15.3.2",
    "sinon": "^1.17.6",
    "stylefmt": "^4.3.1",
    "url-loader": "^0.5.7"
  }
}
